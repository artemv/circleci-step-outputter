    "nyc": "^11.0.0",
    "semantic-release": "^7.0.0"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "release": {
    "analyzeCommits": "freeform-semantic-commit-analyzer",
    "verifyConditions": "condition-circle"
